<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.back.office.entity.AircraftDetails" table="aircraftDetails" catalog="">
        <id name="aircraftId" type="java.lang.Integer">
            <column name="aircraftId" />
            <generator class="identity" />
        </id>
        <property name="aircraftName" type="java.lang.String">
            <column name="aircraftName" length="100" />
        </property>
        <property name="galleyPosition" type="java.lang.String">
            <column name="galleyPosition" length="10" />
        </property>
        <property name="equipmentType" type="java.lang.String">
            <column name="equipmentType" length="30" />
        </property>
        <property name="noOfFullCrts" type="java.lang.Integer">
            <column name="noOfFullCarts" />
        </property>
        <property name="totalNoOfSeats" type="java.lang.Integer">
            <column name="totalNoOfSeats"  />
        </property>
        <property name="active" type="boolean">
            <column name="active" />
        </property>
        <property name="updateDateAndtime" type="java.lang.String">
            <column name="UpdateDateAndtime" length="50" />
        </property>
    </class>
</hibernate-mapping>