<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.back.office.entity.AircraftDetails" table="aircraftdetails" catalog="">
        <id name="aircraftId" type="java.lang.Integer">
            <column name="aircraftId" />
            <generator class="identity" />
        </id>
        <property name="aircraftName" type="java.lang.String">
            <column name="aircraftName" length="100" />
        </property>
        <property name="galleyPosition" type="java.lang.String">
            <column name="galleyPosition" length="10" />
        </property>
        <property name="registrationNumber" type="java.lang.String">
            <column name="registrationNumber" length="30" />
        </property>
        <property name="ecoClassSeatCount" type="java.lang.Integer">
            <column name="ecoClassSeatCount" />
        </property>
        <property name="businessClassSeatCount" type="java.lang.Integer">
            <column name="businessClassSeatCount"  />
        </property>
        <property name="frontFullCarts" type="java.lang.Integer">
            <column name="frontFullCarts"  />
        </property>
        <property name="frontHalfCarts" type="java.lang.Integer">
            <column name="frontHalfCarts"  />
        </property>
        <property name="frontContainers" type="java.lang.Integer">
            <column name="frontContainers"  />
        </property>
        <property name="middleFullCarts" type="java.lang.Integer">
            <column name="middleFullCarts"  />
        </property>
        <property name="middleHalfCarts" type="java.lang.Integer">
            <column name="middleHalfCarts"  />
        </property>
        <property name="middleContainers" type="java.lang.Integer">
            <column name="middleContainers"  />
        </property>
        <property name="rearFullCarts" type="java.lang.Integer">
            <column name="rearFullCarts"  />
        </property>
        <property name="rearHalfCarts" type="java.lang.Integer">
            <column name="rearHalfCarts"  />
        </property>
        <property name="rearContainers" type="java.lang.Integer">
            <column name="rearContainers"  />
        </property>
        <property name="active" type="boolean">
            <column name="active" />
        </property>
        <property name="updateDateAndtime" type="java.lang.String">
            <column name="UpdateDateAndtime" length="50" />
        </property>
        <property name="recordStatus" type="java.lang.Integer">
            <column name="recordStatus"  />
        </property>
    </class>
</hibernate-mapping>