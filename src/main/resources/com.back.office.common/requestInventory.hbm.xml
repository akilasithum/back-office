<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.back.office.entity.RequestInventory" table="request_inventory" catalog="">
        <id name="reqInventoryId" type="java.lang.Integer">
            <column name="reqInventoryId" />
            <generator class="identity" />
        </id>
        <property name="requestedUser" type="java.lang.String">
            <column name="requestedUser" length="30" />
        </property>
        <property name="requestedWareHouse" type="java.lang.String">
            <column name="requestedWareHouse" length="30" />
        </property>
        <property name="requestedFrom" type="java.lang.String">
            <column name="requestedFrom" length="30" />
        </property>
        <property name="requestedDate" type="java.util.Date">
            <column name="requestedDate"  />
        </property>
    </class>
</hibernate-mapping>
