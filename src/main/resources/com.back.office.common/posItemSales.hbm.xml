<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.back.office.entity.PosItemSaleDetail" table="pos_item_sale_details" catalog="">
        <id name="saleOrderId" type="java.lang.Integer">
            <column name="saleOrderId" />
            <generator class="identity" />
        </id>
        <property name="orderId" type="java.lang.String">
            <column name="orderId" length="30" />
        </property>
        <property name="itemId" type="java.lang.String">
            <column name="itemId" length="100" />
        </property>
        <property name="quantity" type="java.lang.Integer">
            <column name="quantity" length="30" />
        </property>
        <property name="price" type="java.lang.Float">
            <column name="price" length="50"/>
        </property>

    </class>

</hibernate-mapping>