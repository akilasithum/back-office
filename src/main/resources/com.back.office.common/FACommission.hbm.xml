<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.back.office.entity.FACommission" table="fa_commission_view" catalog="">
        <id name="id" type="java.lang.String">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="flightNo" type="java.lang.String">
            <column name="flightNo" length="30" />
        </property>
        <property name="flightDate" type="java.util.Date">
            <column name="flightDate"  />
        </property>
        <property name="sector" type="java.lang.String">
            <column name="sector" length="30" />
        </property>
        <property name="totalSale" type="java.lang.Float">
            <column name="totalSale" />
        </property>
        <property name="sifNo" type="java.lang.Integer">
            <column name="sifNo" />
        </property>
        <property name="faCount" type="java.lang.Integer">
            <column name="faCount" />
        </property>
    </class>
</hibernate-mapping>
