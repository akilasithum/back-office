<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.back.office.entity.MonthEndInventory" table="month_end_inventory" catalog="">
        <id name="monthEndInventoryId" type="java.lang.Integer">
            <column name="monthEndInventoryId" />
            <generator class="identity" />
        </id>
        <property name="month" type="java.lang.String">
            <column name="month"  />
        </property>
        <property name="station" type="java.lang.String">
            <column name="station" />
        </property>
        <property name="year" type="java.lang.Integer">
            <column name="year"  />
        </property>
        <property name="user" type="java.lang.String">
            <column name="user" />
        </property>
        <property name="types" type="java.lang.String">
            <column name="types" />
        </property>
    </class>
</hibernate-mapping>
