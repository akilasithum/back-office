<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.back.office.entity.PassengerPurchases" table="pax_details" catalog="">
        <id name="paxId" type="java.lang.Integer">
            <column name="paxId" />
            <generator class="identity" />
        </id>
        <property name="PNR" type="java.lang.String">
            <column name="PNR" length="100"/>
        </property>
        <property name="paxName" type="java.lang.String">
            <column name="paxName" length="30" />
        </property>
        <property name="email" type="java.lang.String">
            <column name="email"  />
        </property>
        <property name="telephoneNo" type="java.lang.String">
            <column name="telephoneNo" />
        </property>
        <property name="frequentFlyerNo" type="java.lang.String">
            <column name="frequentFlyerNo" />
        </property>
        <property name="deparureDate" type="java.util.Date">
            <column name="deparureDate" />
        </property>

        <property name="flightNo" type="java.lang.String">
            <column name="flightNo" length="30" />
        </property>
        <property name="orderId" type="java.lang.String">
            <column name="orderId" length="30" />
        </property>



    </class>

</hibernate-mapping>